# Remove colors
color.tagged           = none
color.recurring        = none
color.blocking         = none
color.calendar.weekend = none

# Actual colors
color.active                   = rgb200 on $COLOR_OBJ
color.label                    = $COLOR_TYPE
color.label.sort               = $COLOR_TYPE
color.overdue                  = $COLOR_OP
color.calendar.overdue         = $COLOR_OP
color.due.today                = $COLOR_OP
color.calendar.due.today       = $COLOR_OP
color.due                      = $COLOR_WARN
color.calendar.due             = $COLOR_WARN
color.tag.Red                  = $COLOR_ERR
color.tag.Blue                 = $COLOR_HINT
color.tag.Purple               = $COLOR_SEARCH_ACTIVE
color.tag.next                 = $COLOR_KEY
color.uda.priority.VL          = gray7
color.uda.priority.L           = gray11
color.uda.priority.            = gray15
color.uda.priority.M           = gray19
color.uda.priority.H           = gray23
color.calendar.today           = $COLOR_KEY

# Colors order
rule.precedence.color = deleted,completed,active,keyword.,tag.next,tag.,project.,overdue,scheduled,due.today,due,blocked,blocking,recurring,tagged,uda.
